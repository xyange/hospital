<?xml version="1.0" encoding="UTF-8"?>



<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.wyzy.hospital.psychology.mapper.DoctorServeMapper">

  <resultMap id="doctorServeMap" type="com.wyzy.hospital.psychology.api.entity.DoctorServe">

                  <id property="id" column="id"/>
      <result property="doctorId" column="doctor_id"/>
                        <result property="serveId" column="serve_id"/>
            </resultMap>
    <select id="selectDoctorServeByServeId" resultMap="doctorServeMap">
        select id,doctor_id,serve_id from wyzy_psychology.doctor_serve where serve_id = #{serveId}
    </select>
</mapper>
