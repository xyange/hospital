<?xml version="1.0" encoding="UTF-8"?>



<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.wyzy.hospital.admin.mapper.PhysicalSymptomTypeMapper">

  <resultMap id="physicalSymptomTypeMap" type="com.wyzy.hospital.admin.api.entity.PhysicalSymptomType">
                  <id property="id" column="id"/>
                        <result property="symptomName" column="symptom_name"/>
                        <result property="delFlag" column="del_flag"/>
                        <result property="isOnline" column="is_online"/>
                        <result property="createTime" column="create_time"/>
                        <result property="updateTime" column="update_time"/>
            </resultMap>
    <select id="pagePhysicalSymptomType" resultMap="physicalSymptomTypeMap">
        select id,symptom_name,del_flag,is_online,create_time,update_time from physical_symptom_type where del_flag = 0 and is_online = 1
    </select>
    <select id="selectBySymptomName" resultMap="physicalSymptomTypeMap">
        select id,symptom_name,del_flag,is_online,create_time,update_time from physical_symptom_type where symptom_name = #{symptomName}
    </select>
</mapper>
